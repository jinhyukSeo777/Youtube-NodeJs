{
  "version": 2,
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/db.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/middlewares.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/views/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/server.js"
    }
  ]
}
