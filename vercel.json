{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "db.js",
      "use": "@vercel/node"
    },
    {
      "src": "middlewares.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    },
    {
      "src": "controllers/**",
      "use": "@vercel/static"
    },
    {
      "src": "models/**",
      "use": "@vercel/static"
    },
    {
      "src": "routers/**",
      "use": "@vercel/static"
    },
    {
      "src": "views/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
